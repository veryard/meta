{
    "name": "veryard/meta",
    "type": "library",
    "description": "Meta",
    "version": "1.0.0",
    "keywords": [
        "veryard",
        "meta tags"
    ],
    "homepage": "https://github.com/veryard/meta",
    "license": "MIT",
    "authors": [
        {
            "name": "Brad Veryard",
            "email": "brad@veryard.dev",
            "homepage": "https://veryard.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Veryard\\Meta\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Veryard\\Meta\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "format": "php-cs-fixer fix --allow-risky=yes"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },

    "config": {
        "sort-packages": true
    }
}
